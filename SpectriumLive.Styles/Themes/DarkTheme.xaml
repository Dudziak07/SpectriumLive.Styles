<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <SolidColorBrush x:Key="PrimaryBackgroundColor" Color="#1E1E1E"/>
    <SolidColorBrush x:Key="SecondaryBackgroundColor" Color="#2D2D2D"/>
    <SolidColorBrush x:Key="PrimaryForegroundColor" Color="#F0F0F0"/>
    <SolidColorBrush x:Key="BorderColor" Color="#505050"/>
    <SolidColorBrush x:Key="AccentColor" Color="#C90094"/>
    <SolidColorBrush x:Key="Disabled.ForegroundColor" Color="#505050"/>
    <SolidColorBrush x:Key="Disabled.BackgroundColor" Color="#303030"/>

    <!-- Button -->
    <SolidColorBrush x:Key="Button.Static.Background" Color="#3F3F3F"/>
    <SolidColorBrush x:Key="Button.Static.Border" Color="#5A5A5A"/>
    <SolidColorBrush x:Key="Button.MouseOver.Background" Color="#555555"/>
    <SolidColorBrush x:Key="Button.MouseOver.Border" Color="#7A7A7A"/>
    <SolidColorBrush x:Key="Button.Pressed.Background" Color="#6A6A6A"/>
    <SolidColorBrush x:Key="Button.Pressed.Border" Color="#909090"/>
    <SolidColorBrush x:Key="Button.Foreground" Color="#FFFFFF"/>

    <!-- GroupBox -->
    <SolidColorBrush x:Key="GroupBox.Border" Color="#505050"/>
    <SolidColorBrush x:Key="GroupBox.Header.Foreground" Color="#F0F0F0"/>

    <!-- MenuItem -->
    <SolidColorBrush x:Key="MenuItem.Static.Background" Color="#2D2D2D"/>
    <SolidColorBrush x:Key="MenuItem.Static.Border" Color="#404040"/>
    <SolidColorBrush x:Key="MenuItem.MouseOver.Background" Color="#3F3F3F"/>
    <SolidColorBrush x:Key="MenuItem.MouseOver.Border" Color="#5A5A5A"/>
    <SolidColorBrush x:Key="MenuItem.Pressed.Background" Color="#505050"/>
    <SolidColorBrush x:Key="MenuItem.Pressed.Border" Color="#7A7A7A"/>
    <SolidColorBrush x:Key="MenuItem.Foreground" Color="#F0F0F0"/>
    <SolidColorBrush x:Key="MenuItem.Submenu.Border" Color="#5A5A5A"/>

    <!-- TextBox -->
    <SolidColorBrush x:Key="TextBox.Static.Background" Color="#2D2D2D"/>
    <SolidColorBrush x:Key="TextBox.Static.Border" Color="#505050"/>
    <SolidColorBrush x:Key="TextBox.Static.Foreground" Color="#F0F0F0"/>
    <SolidColorBrush x:Key="TextBox.MouseOver.Border" Color="#7A7A7A"/>
    <SolidColorBrush x:Key="TextBox.Focus.Border" Color="#C90094"/>
    <SolidColorBrush x:Key="TextBox.Caret" Color="#F0F0F0"/>
    <SolidColorBrush x:Key="TextBox.Selection" Color="#40C90094"/>

    <!-- TextBlock -->
    <SolidColorBrush x:Key="TextBlock.Foreground" Color="#F0F0F0"/>
    
    <!-- Slider -->
    <SolidColorBrush x:Key="Slider.Track.Background" Color="#404040"/>
    <SolidColorBrush x:Key="Slider.SelectionRange.Background" Color="#C90094"/>
    <SolidColorBrush x:Key="Slider.Thumb.Static.Background" Color="#A0A0A0"/>
    <SolidColorBrush x:Key="Slider.Thumb.Static.Border" Color="#ACACAC"/>
    <SolidColorBrush x:Key="Slider.Thumb.MouseOver.Background" Color="#E0E0E0"/>
    <SolidColorBrush x:Key="Slider.Thumb.Pressed.Background" Color="#FFFFFF"/>
    <SolidColorBrush x:Key="Slider.Thumb.Disabled.Background" Color="#606060"/>
    <SolidColorBrush x:Key="Slider.Tick.Fill" Color="#808080"/>

    <!-- ComboBox -->
    <SolidColorBrush x:Key="ComboBox.Static.Background" Color="#3F3F3F"/>
    <SolidColorBrush x:Key="ComboBox.Static.Border" Color="#5A5A5A"/>
    <SolidColorBrush x:Key="ComboBox.Static.Foreground" Color="#F0F0F0"/>
    <SolidColorBrush x:Key="ComboBox.MouseOver.Border" Color="#7A7A7A"/>
    <SolidColorBrush x:Key="ComboBox.Pressed.Border" Color="#C90094"/>
    <SolidColorBrush x:Key="ComboBox.Arrow.Static.Fill" Color="#F0F0F0"/>
    <SolidColorBrush x:Key="ComboBox.Popup.Background" Color="#2D2D2D"/>
    <SolidColorBrush x:Key="ComboBox.Popup.Border" Color="#505050"/>

    <!-- ComboBoxItem -->
    <SolidColorBrush x:Key="ComboBoxItem.MouseOver.Background" Color="#555555"/>
    <SolidColorBrush x:Key="ComboBoxItem.Selected.Background" Color="#C90094"/>
    <SolidColorBrush x:Key="ComboBoxItem.Selected.Foreground" Color="#FFFFFF"/>

    <!-- RadioButton -->
    <SolidColorBrush x:Key="RadioButton.Glyph.Static.Fill" Color="#A0A0A0"/>
    <SolidColorBrush x:Key="RadioButton.Glyph.MouseOver.Fill" Color="#F0F0F0"/>

    <!-- TabControl -->
    <SolidColorBrush x:Key="TabControl.Content.Background" Color="#2D2D2D"/>
    <SolidColorBrush x:Key="TabControl.Content.Border" Color="#505050"/>

    <!-- TabItem -->
    <SolidColorBrush x:Key="TabItem.Static.Background" Color="#3F3F3F"/>
    <SolidColorBrush x:Key="TabItem.Static.Border" Color="#5A5A5A"/>
    <SolidColorBrush x:Key="TabItem.MouseOver.Background" Color="#555555"/>
    <SolidColorBrush x:Key="TabItem.MouseOver.Border" Color="#7A7A7A"/>
    <SolidColorBrush x:Key="TabItem.Selected.Background" Color="#C90094"/>
    <SolidColorBrush x:Key="TabItem.Selected.Foreground" Color="#FFFFFF"/>
    <SolidColorBrush x:Key="TabItem.Selected.Border" Color="#C90094"/>

    <!-- ScrollBar -->
    <SolidColorBrush x:Key="ScrollBar.Track.Static.Background" Color="#2D2D2D"/>
    <SolidColorBrush x:Key="ScrollBar.Static.Background" Color="#3F3F3F"/>
    <SolidColorBrush x:Key="ScrollBar.Static.Border" Color="#5A5A5A"/>
    <SolidColorBrush x:Key="ScrollBar.MouseOver.Background" Color="#555555"/>
    <SolidColorBrush x:Key="ScrollBar.MouseOver.Border" Color="#7A7A7A"/>
    <SolidColorBrush x:Key="ScrollBar.Pressed.Background" Color="#6A6A6A"/>
    <SolidColorBrush x:Key="ScrollBar.Pressed.Border" Color="#909090"/>
    <SolidColorBrush x:Key="ScrollBar.Thumb.Static.Background" Color="#686868"/>
    <SolidColorBrush x:Key="ScrollBar.Thumb.Static.Border" Color="#7A7A7A"/>
    <SolidColorBrush x:Key="ScrollBar.Thumb.MouseOver.Background" Color="#808080"/>
    <SolidColorBrush x:Key="ScrollBar.Thumb.MouseOver.Border" Color="#909090"/>
    <SolidColorBrush x:Key="ScrollBar.Thumb.Pressed.Background" Color="#A0A0A0"/>
    <SolidColorBrush x:Key="ScrollBar.Thumb.Pressed.Border" Color="#ACACAC"/>
    <SolidColorBrush x:Key="ScrollBar.Glyph.Static.Fill" Color="#F0F0F0"/>
    <SolidColorBrush x:Key="ScrollBar.Glyph.MouseOver.Fill" Color="#FFFFFF"/>
    <SolidColorBrush x:Key="ScrollBar.Glyph.Pressed.Fill" Color="#FFFFFF"/>

</ResourceDictionary>